{
    "name": "bryangruneberg/slail",
    "description": "Slack + Tail = SLAIL",
    "keywords": ["slack", "cli", "tail"],
    "homepage": "https://bryangruneberg.com/slail",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/bryangruneberg/slail/issues",
        "source": "https://github.com/bryangruneberg/slail"
    },
    "authors": [
        {
            "name": "Bryan Gruneberg",
            "email": "hello@bryangruneberg.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "jclg/php-slack-bot": "^0.0.7",
        "laravel-zero/framework": "5.7.*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-posix": "0"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["slail"]
}
